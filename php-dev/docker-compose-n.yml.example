nginx:
    image: ibbd/nginx
    container_name: ibbd-nginx
    ports:
        - 80:80
    volumes:
        - /var/www:/var/www
        - /var/log/nginx:/var/log/nginx 
        - /etc/nginx/nginx-conf-v2/products/sites-enabled:/etc/nginx/sites-enabled:ro
        - /etc/nginx/nginx-conf-v2/products/nginx.conf:/etc/nginx/nginx.conf:ro


